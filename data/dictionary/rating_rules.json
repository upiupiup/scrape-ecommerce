{
  "schema_version": "1.0",
  "defaults": {
    "flags": [
      "IGNORECASE",
      "MULTILINE"
    ],
    "min_rating": 1.0,
    "max_rating": 5.0
  },
  "rules": [
    {
      "id": "out_of_5_any",
      "pattern": "(?:\\(|\\[)?\\s*((?:[1-5](?:[.,]5)?)|(?:[1-4]\\s?(?:¬Ω|1/2)))\\s*/\\s*5\\s*(?:\\)|\\])?",
      "example": "4,5/5 mantap",
      "type": "extract",
      "value_group": 1,
      "supports_half": true,
      "postprocess": {
        "replace": {
          ",": "."
        }
      },
      "clamp": [
        1.0,
        5.0
      ],
      "priority": 100
    },
    {
      "id": "bintang_any",
      "pattern": "\\b(?:(?:kasih|beri|dapat|ngasih)\\s*)?(?:bintang(?:nya|ny|\\snya)?\\s*((?:\\d+(?:[.,]\\d+)?)|(?:[1-4]\\s?(?:¬Ω|1/2)))(?:\\+{1,2})?|([1-5])\\s*bintang|(satu|dua|tiga|empat|lima)\\s*bintang)\\b",
      "example": "kasih 5 bintang",
      "type": "extract_multi",
      "value_groups": [
        1,
        2
      ],
      "word_groups": [
        3
      ],
      "word_map": {
        "satu": 1,
        "dua": 2,
        "tiga": 3,
        "empat": 4,
        "lima": 5
      },
      "supports_half": true,
      "postprocess": {
        "replace": {
          ",": "."
        }
      },
      "clamp": [
        1.0,
        5.0
      ],
      "treat_many_as_max": true,
      "priority": 98
    },
    {
      "id": "rating_word_number",
      "pattern": "\\b(rating|rate|nilai|skor|score|grade)\\s*[:\\-]?\\s*([1-5](?:[.,]5)?)\\b",
      "example": "rating: 5",
      "type": "extract",
      "value_group": 2,
      "postprocess": {
        "replace": {
          ",": "."
        }
      },
      "clamp": [
        1.0,
        5.0
      ],
      "priority": 96
    },
    {
      "id": "stars_word_any",
      "pattern": "\\b((?:[1-5])|(?:satu|dua|tiga|empat|lima))\\s*stars?\\b",
      "example": "4 stars overall",
      "type": "extract_or_map",
      "value_group": 1,
      "word_groups": [
        1
      ],
      "word_map": {
        "satu": 1,
        "dua": 2,
        "tiga": 3,
        "empat": 4,
        "lima": 5
      },
      "postprocess": {
        "replace": {
          ",": "."
        }
      },
      "clamp": [
        1.0,
        5.0
      ],
      "priority": 94
    },
    {
      "id": "emoji_stars_any",
      "pattern": "(?:\\b(?:‚≠ê(?:\\uFE0F)?|‚òÖ)\\s*[xX√ó]\\s*([1-5])\\b)|(?:(?:‚≠ê(?:\\uFE0F)?|‚òÖ|üåü|‚ú©|‚úÆ|‚úØ){2,10})",
      "example": "‚≠ê x5",
      "type": "emoji_or_mult",
      "emojis": [
        "‚≠ê",
        "‚òÖ",
        "üåü",
        "‚ú©",
        "‚úÆ",
        "‚úØ"
      ],
      "mult_group": 1,
      "min_count": 2,
      "max_count": 5,
      "priority": 92
    },
    {
      "id": "number_of_five",
      "pattern": "\\b([1-5])\\s*(?:of|out\\s+of|dr|dari)\\s*5\\b",
      "example": "4 of 5 for design",
      "type": "extract",
      "value_group": 1,
      "clamp": [
        1.0,
        5.0
      ],
      "priority": 90
    }
  ],
  "blacklist": [
    {
      "id": "star_bullet_single",
      "pattern": "^[\\s]*[‚òÖ‚≠ê]",
      "reason": "Bullet list (satu bintang di awal baris), bukan rating."
    }
  ]
}